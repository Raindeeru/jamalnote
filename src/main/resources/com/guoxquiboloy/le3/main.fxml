<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.guoxquiboloy.le3.MainController">
   <children>
      <Button fx:id="newFileButton" mnemonicParsing="false" onAction="#OpenNewFile" text="New File" />
      <Button fx:id="loadButton" mnemonicParsing="false" onAction="#LoadFile" text="Load File" />
      <Button fx:id="closeButton" onAction="#Close" text="Close" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
